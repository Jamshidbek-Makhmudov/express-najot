<body>
  <header data-bs-theme="dark">
    <nav class="navbar navbar-expand-md navbar-dark bg-dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">Carousel</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse"
          aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav me-auto mb-2 mb-md-0">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="#">Add Video</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/api/login">Login</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/api/register">Register</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/api/logout">logout</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>

  <main>
    <div class="container marketing">
      <div class="row">
        <%users.forEach((user)=> {%>
          <div class="col-lg-4">
            <img src="/<%=user.image%>" width="200px" class="rounded-circle" alt="" />
            <h2 class="fw-normal">
              <%=user.fullName%>
            </h2>

            <p>
              <a class="btn btn-secondary" href="/api/videos/<%=user.id%>">View details »</a>
            </p>
          </div>
          <%})%>
      </div>
    </div>
  </main>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
    crossorigin="anonymous"></script>
</body>

</html>